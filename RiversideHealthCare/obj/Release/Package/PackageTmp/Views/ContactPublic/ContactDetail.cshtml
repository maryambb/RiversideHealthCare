@model RiversideHealthCare.Models.contact_list

@{
    ViewBag.Title = "ContactDetail";
   
}
<div class="container">
    <h2>More Contact Information</h2>
    <table>
        <thead>
            <tr>
                <th>Department</th>
                <th>Detail</th>
                <th>Location</th>
                <th>Phone</th>
                <th>Fax</th>
                <th>Haed</th>
                <th></th>
            </tr>
        </thead>
        <tr>
            <td>@Html.DisplayFor(x => x.department)</td>
            <td>@Html.DisplayFor(x => x.detail)</td>
            <td>@Html.DisplayFor(x => x.location)</td>
            <td>@Html.DisplayFor(x => x.phone)</td>
            <td>@Html.DisplayFor(x => x.fax)</td>
            <td>@Html.DisplayFor(x => x.head)</td>
        </tr>
    </table>
    </div>

    @section scripts {
        @Scripts.Render("~/bundles/jqueryval")
    }
